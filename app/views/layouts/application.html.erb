<!DOCTYPE html>
<html>
<head>
  <title>Intranet</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="container">
<%= render 'home/header' %>
</div>

<div class="container">
<p role="alert">
<%= notice %>
<%= alert %>
</p>
</div>

<div class="container">
<div class="jumbotron">
<%= yield %>
</div>
</div>

<div class="container">
<div class="footer">
<%= render 'home/footer' %>
</div>
</div>
</body>
</html>
