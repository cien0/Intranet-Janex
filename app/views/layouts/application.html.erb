<!DOCTYPE html>
<html>
<head>
  <title>Intranet</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="container">
<div class="nav navbar-nav navbar-left" id="bs-example-navbar-collapse-l">

<%= render 'home/header' %>

</div>
</div>
<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>

<div class="container-fluid">
<div class="jumbotron">
<%= yield %>
</div>
</div>

<div class="footer">
<%= render 'home/footer' %>
</div>
</body>
</html>
